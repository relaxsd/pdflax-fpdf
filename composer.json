{
  "name": "relaxsd/pdflax-fpdf",
  "description": "Fpdf implementation for Pdflax.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martijn Kriek",
      "email": "martijn.kriek@relax-software.nl"
    }
  ],
  "require": {
    "anouar/fpdf": "~2.1.0",
    "relaxsd/pdflax": "0.8.*",
    "relaxsd/php-stylesheets": "2.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "git@github.com:relaxsd/laravel-fpdf.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Relaxsd\\Pdflax\\Fpdf\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8.x-dev"
    }
  }
}
